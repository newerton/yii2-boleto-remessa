{
  "name": "newerton/yii2-boleto-remessa",
  "description": "Biblioteca com boletos e sistema de remessa e retorno para o Yii2 Framework",
  "keywords": [
    "newerton",
    "yii2",
    "yii2-boleto-remessa"
  ],
  "homepage": "https://github.com/newerton/yii2-boleto-remessa",
  "license": "MIT",
  "authors": [
    {
      "name": "Newerton Vargas de Araujo",
      "email": "newerton.araujo@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "itbz/fpdf": "^1.7",
    "nesbot/carbon": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "orchestra/testbench": "3.2"
  },
  "autoload": {
    "psr-4": {
      "Newerton\\Yii2Boleto\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Newerton\\Yii2Boleto\\Tests\\": "tests"
    }
